#include<stdio.h>
int lcm(int a,int b);
int main()
{
 int n1,n2;
 scanf("%d %d",&n1,&n2);
 if(n1>n2)
 int t=lcm(n2,n1);
 else
 int t=lcm(n1,n2);
 printf("%d",lcm);
 return 0;
}
int lcm(int a,int b)
{
 static int multiple=0;
 multiple+=b;
 if((multiple%a==0)&&(multiple%b==0))
 return multiple;
 else
 return lcm(a,b);
}
